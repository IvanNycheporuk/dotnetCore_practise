@{
    var news = ViewData["News"] as List<News>;
}

@section Table {
    <table>
        @foreach (var item in news)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.IsFake</td>
                <td>@item.Title</td>
                <td>@item.AuthorName</td>
            </tr>
        }
    </table>
}
