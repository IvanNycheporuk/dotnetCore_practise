@{ 
    var data = ViewData["Characters"] as List<Character>;
}

<table>
    @foreach (var item in data)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Age</td>
            <td>@item.Name</td>            
        </tr>
    }
</table>

<p>Character are going to be displayed here</p>